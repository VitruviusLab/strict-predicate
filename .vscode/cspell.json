{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"language": "en-GB",
	"allowCompoundWords": true,
	"dictionaries": ["softwareTerms", "typescript", "node", "html", "css", "bash", "npm"],
	"words": [],
	"ignoreWords": ["sonarjs"],
	"flagWords": ["hte"],
	"ignorePaths": [
	  ".eslintrc.json",
	  "tsconfig.json",
	  ".mocharc.json",
	  ".gitignore",
	  "**/*.log",
	  "package.json",
	  "**/node_modules/**",
	  "**/.husky/**",
	  "**/.yarn/**",
	  "**/.stryker-tmp/**",
	  "**/stats/**",
	  "**/docs/**",
	  "**/documentations/**",
	  "**/reports/**",
	  "**/coverage/**",
	  "**/build/**",
	  "**/dist/**",
	  "**/lib/**",
	  "**/.git/**",
	  "./src/config/**",
	  "./src/configs/**",
	  "vscode-extension",
	  ".github/CODEOWNERS",
	  ".vscode",
	  "*.lock",
	  "CHANGELOG.md"
	]
  }
